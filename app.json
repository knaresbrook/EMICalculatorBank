{
  "expo": {
    "name": "emicalculator",
    "slug": "emicalculator",
    "version": "6.0.0",
    "orientation": "portrait",
    "icon": "./assets/applicationicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/emisplashimage.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mnkltd.emicalculator",
      "buildNumber": "2"
    },
    "android": {
      "versionCode": 6,
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/applicationicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mnkltd.emicalculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8bf066c8-ae03-4585-b219-480876e73c01"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ]
  }
}
